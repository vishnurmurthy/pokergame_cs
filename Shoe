class Shoe {

	private String type ="";	
	private String brand ="";	
	private String Color= "";
	private String size= "";
	private String price="";

	public User() {}

	public User (String newType, String newBrand, String newColor, String newSize, String newPrice) { 

		setType (newType);
		setBrand (newBrand);
		setColor(newColor);
		setSize(newSize);
		setPrice(newPrice);
	}

	public String getType() {					
		return type;
	}

	public void setType (String type) {		
		this.type = type;
	}

	public String getBrand() {					
		return brand;
	}

	public void setBrand (String brand) {		
		this.brand = brand;
	}
	
	public void getColor(){
	  return color;
	}
	
	public void setColor(String color){
	  this.color= color;
	}
	
	public void getSize(){
	  return size;
	}
  
  public void setSize(String size){
    this.size= size;
  }
  
  public void getPrice(){
    return price;
  }
  
  public void setPrice(){
    this.price= price;
  }
  

}
